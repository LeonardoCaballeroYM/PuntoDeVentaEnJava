	<queryString>
		<![CDATA[SELECT
     inv_salidas.`cod_producto` AS inv_salidas_cod_producto,
     inv_salidas.`producto` AS inv_salidas_producto,
     inv_salidas.`precio` AS inv_salidas_precio,
     inv_salidas.`cantidad` AS inv_salidas_cantidad,
     ventas.`factura` AS ventas_factura,
     ventas.`id_vendedor` AS ventas_id_vendedor,
     ventas.`total_vendido` AS ventas_total_vendido,
     ventas.`importe` AS ventas_importe,
     ventas.`cambio` AS ventas_cambio,
     ventas.`fecha` AS ventas_fecha,
     vendedor.`cliente` AS vendedor_cliente,
     vendedor.`fecha` AS vendedor_fecha,
     usuario.`nombre` AS usuario_nombre,
     usuario.`apellido` AS usuario_apellido,
     ventas.`id_venta` AS ventas_id_venta
FROM
     `ventas` ventas INNER JOIN `inv_salidas` inv_salidas ON ventas.`id_venta` =  inv_salidas.`id_venta`
     INNER JOIN `vendedor` vendedor ON ventas.`id_vendedor` = vendedor.`id`
     INNER JOIN `usuario` usuario ON vendedor.`id_empleado` = usuario.`id`
WHERE ventas.factura = $P!{factura}]]>
	</queryString>
