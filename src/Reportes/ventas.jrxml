	<queryString>
		<![CDATA[SELECT
     ventas.`factura` AS ventas_factura,
     inv_salidas.`cod_producto` AS inv_salidas_cod_producto,
     inv_salidas.`producto` AS inv_salidas_producto,
     inv_salidas.`precio` AS inv_salidas_precio,
     inv_salidas.`cantidad` AS inv_salidas_cantidad,
     inv_salidas.`total` AS inv_salidas_total
FROM
     `inv_salidas` inv_salidas INNER JOIN `ventas` ventas ON inv_salidas.`id_venta` = ventas.`factura`
    where ventas.`factura` = inv_salidas.`id_venta` and ventas.fecha = $P{fecha}]]>
